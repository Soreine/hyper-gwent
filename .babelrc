{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 8 Chrome versions, last 4 Firefox versions, Chrome >= 55, Firefox >= 53",
        "useBuiltIns": "usage",
        "corejs": "3.0.0"
      }
    ],
    [
      "@babel/preset-react",
      {
        "pragma": "createElement"
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
